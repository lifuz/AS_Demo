<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lifuz.testvolley" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SimpleActivity"
            android:label="@string/title_activity_simple" >
        </activity>
        <activity
            android:name=".JsonActivity"
            android:label="@string/title_activity_json" >
        </activity>
        <activity
            android:name=".Json2Activity"
            android:label="@string/title_activity_json2" >
        </activity>
        <activity
            android:name=".ImageActivity"
            android:label="@string/title_activity_image" >
        </activity>
        <activity
            android:name=".Image2Acitivity"
            android:label="@string/title_activity_image2_acitivity" >
        </activity>
        <activity
            android:name=".Image3_Activity"
            android:label="@string/title_activity_image3_" >
        </activity>
        <activity
            android:name=".CustomActivity"
            android:label="@string/title_activity_custom" >
        </activity>
        <activity android:name=".Custom2Activity"/>
    </application>

</manifest>
